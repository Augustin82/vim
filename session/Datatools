silent only
cd ~/WoW/datatools.bo
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +55 app/utils/apiRequest.js
