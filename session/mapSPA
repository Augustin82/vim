let SessionLoad = 1
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/Spottt/mapSPA
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +1 src/Ads.elm
badd +1 src/AppAds/View.elm
badd +47 src/AppDirectory/List.elm
badd +167 src/AppDirectory/Styles.elm
